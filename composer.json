{
  "name": "fkrauthan/wp-mpdf",
  "description": "Print Wordpress posts as PDF",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Florian Krauthan",
      "email": "mail@fkrauthan.de"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "composer/installers": "~2.0",
    "mpdf/mpdf": "v8.2.*",
    "geshi/geshi": "v1.0.*",
    "psr/log": "^1.0.0"
  },
  "archive": {
    "exclude": [
      ".gitignore",
      "dev",
      ".idea",
      ".github"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
